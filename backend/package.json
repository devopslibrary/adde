{
  "name": "adde-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "cpy '**/*' '!coverage' '!**/*.ts' ../dist/src --cwd=src/ --no-overwrite --parents",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV='' nest start",
    "start:dev": "NODE_ENV='' nest start --watch",
    "serve": "NODE_ENV='test' nest start --watch",
    "start:debug": "NODE_ENV='' nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^7.0.5",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.5",
    "@nestjs/graphql": "^7.0.13",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/typeorm": "^7.0.0",
    "@octokit/webhooks": "^7.0.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/json-schema": "^7.0.4",
    "ejs": "^3.0.1",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "jwks-rsa": "^1.7.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "readdirsync2": "^1.0.2",
    "recursive-readdir": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "simple-git": "^1.131.0",
    "tmp": "^0.1.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.22",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "cpy-cli": "^3.1.0",
    "del-cli": "^3.0.0",
    "jest": "^25.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.0.1",
    "supertest": "^4.0.2",
    "swagger-parser": "^9.0.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/coverage/",
      "/cache/",
      "/dist/",
      "/test/",
      "/src/main.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
